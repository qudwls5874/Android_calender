<?xml version="1.0" encoding="utf-8"?>
<!-- money 추가 Row -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="250dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@android:color/transparent">


    <LinearLayout
        android:id="@+id/item_tel_d_linearLayout"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="horizontal"
        android:clickable="true"
        android:focusable="true">

        <View
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:gravity="center"/>

        <TextView
            android:id="@+id/item_tel_d_textView"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_gravity="center"
            android:textSize="11dp"
            android:textColor="@color/textNormalColor"
            android:background="@android:color/transparent"
            android:text="타입"/>


        <LinearLayout
            android:id="@+id/item_tel_d_linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/BackBaseInputBoxColor"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/BackBaseInputBoxColor"/>

                <TextView
                    android:id="@+id/item_tel_d_textView2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center|start"
                    android:layout_marginLeft="10dp"
                    android:textSize="11dp"
                    android:textColor="@color/textNormalColor"
                    android:background="@android:color/transparent"
                    android:inputType="number"
                    android:text="0"/>


            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

        </LinearLayout>



    </LinearLayout>

</LinearLayout>